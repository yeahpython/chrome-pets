{
  "name": "Emoji Pet",
  "description": "Get your own cute emoji pet to explore the internet with you!",
  "version": "1.0",
  "permissions": [
    "storage",
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "css": ["mystyles.css"],
      "js": ["third-party/jquery-1.12.0.min.js", "myscript.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts":["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/emoji-19.png",
      "38": "images/emoji-38.png"
    }
  },
